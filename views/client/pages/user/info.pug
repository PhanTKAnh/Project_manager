extends ../../layouts/default.pug
include ../../mixins/box-head.pug

block main
    
    .container.my-3
        .row 
            .col-12 
               +box-head(pageTitle) 
        .row 
            .col-12 
            if(user.fullName)
                div(class="mb-4") Họ và tên : #{user.fullName} 
        .row 
            .col-12 
            if(user.email)
                div(class="mb-4") Email: #{user.email}